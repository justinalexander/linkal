= form_for(current_user.user_organizations.find_by_venue_id(venue), html: { method: :delete }, remote: true) do |f|
  = f.hidden_field :venue_id
  %h5.ui-li-header
    = venue.name
  %p.ui-li-desc
    = venue.category_name
  %p.ui-li-aside
    = f.submit "Unfollow", "data-inline" => "true", "data-mini" => "true"
